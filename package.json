		"vitest": "^0.29.2"
	},
	"type": "module",
	"dependencies": {
		"@stripe/stripe-js": "^1.48.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@types/jquery": "^3.5.29",
		"@unpic/svelte": "^0.0.50",
		"directory-tree": "^3.5.1",
		"pocketbase": "^0.12.1",
		"svelte-markdown": "^0.4.1",
		"tsx": "^4.7.0",
		"tw-elements": "1.0.0-beta1"
	}
